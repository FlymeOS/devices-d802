<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.lge.wapservice" platformBuildVersionCode="21" platformBuildVersionName="5.0.2-1509316167955">
    <uses-permission android:name="com.android.providers.syncml.permission.READ_SYNCML_PROFILE"/>
    <uses-permission android:name="com.android.providers.syncml.permission.WRITE_SYNCML_PROFILE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.UPDATE_APP_OPS_STATS"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.INTERACT_ACROSS_USERS"/>
    <uses-permission android:name="android.permission.MANAGE_USERS"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:supportsRtl="true">
        <meta-data android:name="com.lge.theme" android:value="Theme_LGE_White"/>
        <activity android:name=".prov.StoreProvisioningActivity"/>
        <activity android:configChanges="keyboardHidden|orientation" android:excludeFromRecents="true" android:launchMode="singleTask" android:name=".prov.ProvisioningValidation" android:theme="@style/Theme.NoTitleBar"/>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name=".prov.ProvisioningFailed" android:theme="@style/Translucent_Dialog"/>
        <provider android:authorities="senderlist" android:exported="true" android:name=".settings.SenderListProvider"/>
        <provider android:authorities="com.lge.wapservice.provider.settings" android:exported="true" android:name=".settings.SettingsProvider"/>
        <activity android:label="@string/title_service_message_security" android:name=".settings.WapServiceMessageSetting">
            <intent-filter>
                <action android:name="com.lge.mms.START_SERVICE_MESSAGE_SETTING"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_service_message_security" android:name=".settings.WapServiceMessageSecuritySetting">
            <intent-filter>
                <action android:name="com.lge.mms.START_SERVICE_MESSAGE_SECURITY_SETTING"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_service_message_security_set" android:name=".settings.ServiceSecuritySetActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name=".utils.ReceiveDialog" android:theme="@style/Translucent_Dialog"/>
        <activity android:name=".settings.SenderListActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <action android:name="com.lge.wapservice.action.SHOW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".settings.SenderDeleteActivity">
            <intent-filter>
                <action android:name="android.intent.action.DELETE"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:mimeType="vnd.android.cursor.item/vnd.lge.trustedlist"/>
                <data android:mimeType="vnd.android.cursor.item/vnd.lge.untrustedlist"/>
                <data android:mimeType="vnd.android.cursor.dir/vnd.lge.trustedlist"/>
                <data android:mimeType="vnd.android.cursor.dir/vnd.lge.untrustedlist"/>
            </intent-filter>
        </activity>
        <activity android:name=".settings.SenderMoveActivity">
            <intent-filter>
                <action android:name="com.lge.wapservice.MOVE"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:mimeType="vnd.android.cursor.item/vnd.lge.trustedlist"/>
                <data android:mimeType="vnd.android.cursor.item/vnd.lge.untrustedlist"/>
                <data android:mimeType="vnd.android.cursor.dir/vnd.lge.trustedlist"/>
                <data android:mimeType="vnd.android.cursor.dir/vnd.lge.untrustedlist"/>
            </intent-filter>
        </activity>
        <activity android:name=".settings.SenderEditor">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <action android:name="android.intent.action.EDIT"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:mimeType="vnd.android.cursor.item/vnd.lge.trustedlist"/>
                <data android:mimeType="vnd.android.cursor.item/vnd.lge.untrustedlist"/>
                <data android:mimeType="vnd.android.cursor.dir/vnd.lge.trustedlist"/>
                <data android:mimeType="vnd.android.cursor.dir/vnd.lge.untrustedlist"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.INSERT"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:mimeType="vnd.android.cursor.dir/vnd.lge.trustedlist"/>
                <data android:mimeType="vnd.android.cursor.dir/vnd.lge.untrustedlist"/>
            </intent-filter>
        </activity>
        <activity android:name=".settings.SenderListUpdateActivity">
            <intent-filter>
                <action android:name="com.lge.wapservice.senderlistupdate"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:mimeType="vnd.android.cursor.item/vnd.lge.trustedlist"/>
                <data android:mimeType="vnd.android.cursor.item/vnd.lge.untrustedlist"/>
                <data android:mimeType="vnd.android.cursor.dir/vnd.lge.trustedlist"/>
                <data android:mimeType="vnd.android.cursor.dir/vnd.lge.untrustedlist"/>
            </intent-filter>
        </activity>
        <service android:name=".push.PushExpiration"/>
        <service android:name=".push.PushValidation"/>
        <activity android:name=".push.PushViewer"/>
        <receiver android:enabled="true" android:name=".push.PushReceiver">
            <intent-filter>
                <action android:name="android.provider.Telephony.WAP_PUSH_RECEIVED"/>
                <data android:mimeType="application/vnd.wap.sic"/>
                <data android:mimeType="application/vnd.wap.slc"/>
                <data android:mimeType="lgeWapService/start.push.expiration.service"/>
                <data android:mimeType="lgeWapService/stop.push.expiration.service"/>
                <data android:mimeType="lgeWapService/clear.push.notification"/>
            </intent-filter>
        </receiver>
        <receiver android:enabled="true" android:name=".prov.ProvisioningPushReceiver">
            <intent-filter>
                <action android:name="android.provider.Telephony.WAP_PUSH_RECEIVED"/>
                <data android:mimeType="application/vnd.wap.connectivity-wbxml"/>
                <data android:mimeType="lgeWapService/clear.prov.notification"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.cp.install"/>
            </intent-filter>
        </receiver>
        <receiver android:enabled="true" android:name=".push.HomeReceiver">
            <intent-filter>
                <action android:name="android.intent.action.LAUNCH_BROWSER"/>
            </intent-filter>
        </receiver>
    </application>
    <permission android:name="android.permission.BROADCAST_WAP_PUSH" android:permissionGroup="android.permission-group.MESSAGES" android:protectionLevel="signature"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.WRITE_APN_SETTINGS"/>
    <uses-permission android:name="android.permission.RECEIVE_WAP_PUSH"/>
    <uses-permission android:name="android.permission.WRITE_SMS"/>
    <uses-permission android:name="android.permission.READ_SMS"/>
    <uses-permission android:name="com.android.browser.permission.READ_HISTORY_BOOKMARKS"/>
    <uses-permission android:name="com.android.browser.permission.WRITE_HISTORY_BOOKMARKS"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>
    <uses-permission android:name="android.permission.READ_SETTINGS"/>
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
</manifest>
